# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2

[*.{cs,csx}]
indent_size = 4

[*.ps1]
indent_size = 4

[*.{yaml,yml}]
indent_size = 2

[*.json]
indent_size = 4

[*.asmdef]
indent_size = 4
